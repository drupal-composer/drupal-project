{
    "name": "frmwrk/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "FRMWRK",
            "role": "Development"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://packages.frmwrk.nl"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=7.2",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/adminimal_admin_toolbar": "^1.9",
        "drupal/adminimal_theme": "^1.5",
        "drupal/advagg": "^4.0",
        "drupal/autosave_form": "^1.0",
        "drupal/berf": "1.x-dev",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/block_blacklist": "^1.0",
        "drupal/config_filter": "^1.4",
        "drupal/config_ignore": "^2.1",
        "drupal/config_split": "^1.4",
        "drupal/console": "^1.0.2",
        "drupal/consumer_image_styles": "^3.0@RC",
        "drupal/content_lock": "^1.0@alpha",
        "drupal/core": "^8.7.0",
        "drupal/devel": "^2.1",
        "drupal/devel_php": "^1.1",
        "drupal/easy_breadcrumb": "^1.12",
        "drupal/field_defaults": "^1.1",
        "drupal/field_group": "^1.0",
        "drupal/field_tools": "^1.0@alpha",
        "drupal/flood_unblock": "^1.5",
        "drupal/focal_point": "^1.2",
        "drupal/graphql": "4.x-dev",
        "drupal/honeypot": "^1.29",
        "drupal/hook_event_dispatcher": "^1.25",
        "drupal/image_widget_crop": "^2.2",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jsonapi_extras": "^3.8",
        "drupal/layout_builder_restrictions": "^2.1",
        "drupal/layout_library": "^1.0@beta",
        "drupal/length_indicator": "^1.0@RC",
        "drupal/linkit": "^4.3",
        "drupal/login_security": "^1.5",
        "drupal/mail_login": "^1.0",
        "drupal/menu_admin_per_menu": "^1.0",
        "drupal/metatag": "^1.8",
        "drupal/paragraphs": "^1.8",
        "drupal/paragraphs_features": "^1.6",
        "drupal/password_policy": "^3.0@alpha",
        "drupal/path_redirect_import": "^1.0@beta",
        "drupal/pathauto": "^1.4",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/real_aes": "^2.2",
        "drupal/redirect": "^1.4",
        "drupal/restui": "^1.17",
        "drupal/scheduler": "^1.0",
        "drupal/scheduler_content_moderation_integration": "^1.1",
        "drupal/seckit": "^1.2",
        "drupal/select2": "^1.4",
        "drupal/session_limit": "^1.0@beta",
        "drupal/simple_oauth": "^3.16",
        "drupal/simple_sitemap": "^3.2",
        "drupal/stage_file_proxy": "^1.0@beta",
        "drupal/subrequests": "^2.2",
        "drupal/swiftmailer": "^1.0@beta",
        "drupal/taxonomy_access_fix": "^2.6",
        "drupal/taxonomy_menu": "^3.4",
        "drupal/taxonomy_term_depth": "^2.3",
        "drupal/twig_tweak": "^2.3",
        "drupal/username_enumeration_prevention": "^1.0@beta",
        "drupal/video_embed_field": "^2.1",
        "drupal/webform": "^5.4",
        "drush/drush": "^9.0.0",
        "frmwrk/frmwrk_decoupled": "dev-master",
        "npm-asset/select2": "^4.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "vlucas/phpdotenv": "^2.0",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drupal/coder": "^8.3",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "phpmd/phpmd": "^2.6",
        "phpro/grumphp": "^0.15.2",
        "sebastian/phpcpd": "^3.0",
        "sensiolabs/security-checker": "^5.0",
        "squizlabs/php_codesniffer": "^3.4",
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "patches": {
            "drupal/code" : {
                "3055443 - Switch to a memory backend for running the database updates" : "https://www.drupal.org/files/issues/2019-07-22/3055443-2-37.patch"
            },
            "drupal/rabbit_hole": {
                "2900548 - Add commerce products" : "https://www.drupal.org/files/issues/commerce_support-2900548-3.patch"
            },
            "drupal/inline_entity_form" : {
                "2927884 - Remove revision log" : "https://www.drupal.org/files/issues/2019-07-16/inline_entity_form-remove_log_message-2927884-6.patch"
            },
            "drupal/entity_browser" : {
                "2858438 - Provide inline entity form FieldWidgetDisplay" : "https://www.drupal.org/files/issues/2858438_6.patch",
                "2997747 - The View widget should filter based on field settings" : "https://www.drupal.org/files/issues/2018-09-06/2997747-2.patch"
            }
        }
    },
    "replace": {
        "bower-asset/jquery": "*",
        "bower-asset/jqueryui": "*",
        "bower-asset/backbone": "*",
        "bower-asset/underscore": "*",
        "npm-asset/jquery": "*",
        "npm-asset/jqueryui": "*",
        "npm-asset/backbone": "*",
        "npm-asset/underscore": "*",
        "npm-asset/fortawesome--fontawesome-free": "*"
    }
}
